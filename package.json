{
  "name": "oak-tree-house",
  "version": "0.1.0",
  "description": "My New Personal Homepage",
  "scripts": {
    "build": "vuepress build site",
    "dev": "vuepress dev --no-cache site",
    "decrypt": "site/.vuepress/plugins/encrypt/cli.js --source-dir site --key-file keys.json decrypt",
    "encrypt": "site/.vuepress/plugins/encrypt/cli.js --source-dir site --key-file keys.json encrypt",
    "lint": "npm run lint:before && npm run lint:after",
    "lint:before": "npm run lint:js && npm run lint:md:before",
    "lint:after": "npm run lint:md:after",
    "lint:js": "eslint packages site --ext .js,.vue",
    "lint:md:before": "markdownlint -r \"markdownlint/before/*.js\" \"site/**/*.md\"",
    "lint:md:after": "rm external-link-list.txt -f && markdownlint -r \"markdownlint/after/*.js\" \"site/**/*.md\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunziping2016/oak-tree-house.git"
  },
  "author": "Sun Ziping",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sunziping2016/oak-tree-house/issues"
  },
  "homepage": "https://github.com/sunziping2016/oak-tree-house#readme",
  "devDependencies": {
    "@oak-tree-house/vuepress-theme-vuetify-blog": "^0.1.4",
    "@vuepress/plugin-blog": "^1.8.1",
    "@vuepress/plugin-medium-zoom": "^1.2.0",
    "aes-js": "^3.1.2",
    "chalk": "^3.0.0",
    "commander": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue-libs": "^4.0.0",
    "fs-extra": "^8.1.0",
    "husky": "^4.0.7",
    "markdown-it": "^10.0.0",
    "markdown-it-attrs": "3.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-mathjax": "^2.0.0",
    "markdown-it-task-checkbox": "^1.0.6",
    "markdownlint": "^0.18.0",
    "markdownlint-cli": "^0.21.0",
    "rss": "^1.2.2",
    "vue-feather-icons": "^5.0.0",
    "vuepress": "^1.2.0",
    "vuepress-plugin-container": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
